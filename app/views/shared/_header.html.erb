  <p><%= f.label :room_name_or_room_introduction_or_price_or_adress_cont, 'キーワード検索' %><%= f.search_field :room_name_or_room_introduction_or_price_or_adress_cont %></p>
  <br>
  <%= f.submit '検索' %>
<% end %></header>