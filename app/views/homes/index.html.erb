<h1>Homes#index</h1>
<h1>検索</h1>
<%= search_form_for @q, url: search_path do |f| %>
  <p><%= f.label :adress_cont, 'エリア検索' %><%= f.search_field :adress_cont %></p>
  <p><%= f.label :room_name_or_room_introduction_or_price_or_adress_cont, 'キーワード検索' %><%= f.search_field :room_name_or_room_introduction_or_price_or_adress_cont %></p>
  <br>
  <%= f.submit '検索' %>
<% end %>

<% if user_signed_in? %>
  <%= link_to "ルーム登録", rooms_new_path %>
  <%= link_to "予約一覧", reservations_path %>
  <%= link_to "登録済みルーム一覧", users_show_path(current_user) %>
  <%= link_to "設定", edit_user_registration_path %>
  <%= link_to " ログアウト", destroy_user_session_path, method: :delete %>
<% else %>
  <%= link_to "ログイン", new_user_session_path %>
  <%= link_to "新規登録", new_user_registration_path %>
<% end %>  


