<h1>Rooms#index</h1>
<% @rooms.each do |obj| %></p>
    <p>========================================</p>
      <p>ID:<%= obj.id %></p>
      <p>ルーム名:<%= obj.room_name %></p>
      <p>ルーム紹介:<%= obj.room_introduction %></p>
      <p>料金:<%= obj.price %></p>
      <p>住所:<%= obj.adress %></p>
      <p>user_id:<%= obj.user_id %></p>
      <div>
        <% if obj.image.attached? %>
          <%= image_tag obj.image, class:"img" %>
        <% end %>        
      </div>
    <p>========================================</p>
<% end %>

